{
  "name": "johnny5",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "gen-cert": "openssl req -nodes -x509 -newkey rsa:2048 -keyout keytmp.pem -out cert.pem -days 365 && openssl rsa -in keytmp.pem -out key.pem",
    "first-time": "yarn compile && yarn gen-cert",
    "start": "sudo node index.js"
  },
  "dependencies": {
    "dicer": "^0.2.5",
    "gamepad": "^1.6.0",
    "johnny-five": "^1.0.0",
    "mustache": "^3.0.1",
    "raspi-io": "^9.0.0",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "@types/gamepad": "^1.5.0",
    "@types/johnny-five": "^0.0.38",
    "@types/mustache": "^0.8.32",
    "@types/socket.io": "^2.1.0",
    "typescript": "^3.1.6"
  }
}
